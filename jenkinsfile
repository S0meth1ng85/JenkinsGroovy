node {
    stage('SCM') {
	echo 'Gathering code from version control'
	git branch: '${branch}', url: 'https://github.com/S0meth1ng85/JenkinsGroovy.git'
    }
    stage('Build') {
    try{
        echo 'Building....'
	sh 'dotnet --version'
	sh "dotnet build ConsoleApp1"
	sh 'Building New Feature'
	releasenotes()
    }catch(ex){
    	    echo 'Something went wrong'
	    echo ex.toString();
	    currentBiild.result = 'FAILURE'
	    }
	    finally{
	    //cleanup
	    }
}	    
    stage('Test') {
        echo 'Testing....'
    }
    stage('Deploy') {
        echo 'Deploying....'
    }
}
